#include <stdio.h>

int main() {
    int valor;
    
    printf("Digite o valor total em reais: ");
    scanf("%d", &valor);
    
    int nota100 = valor / 100;
    valor %= 100;
    
    int nota50 = valor / 50;
    valor %= 50;
    
    int nota10 = valor / 10;
    valor %= 10;
    
    int nota5 = valor / 5;
    valor %= 5;
    
    int nota1 = valor;
    
    printf("Notas necess√°rias:\n");
    printf("100 reais: %d\n", nota100);
    printf("50 reais: %d\n", nota50);
    printf("10 reais: %d\n", nota10);
    printf("5 reais: %d\n", nota5);
    printf("1 real: %d\n", nota1);
    
    return 0;
}
